	<input:Combo x="268" y="28" id="cmbStatusEnvio" labelField="label" width="100" editable="false" inserirItemOpcional="true" labelItemOpcional="TODOS"/>
	
	<Rotulo text="Data InÃ­cio:" x="360" y="30"  width="70" textAlign="right" />
