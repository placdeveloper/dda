<?xml version="1.0" encoding="utf-8"?>
<BaseModulo xmlns:mx="http://www.adobe.com/2006/mxml" focusEnabled="false"
			xmlns="br.com.bancoob.componentes.*"
			xmlns:grid="br.com.bancoob.componentes.grid.*"
			xmlns:titulo="br.com.bancoob.componentes.titulo.*"
			xmlns:componentes="br.com.bancoob.componentes.*"
			xmlns:input="br.com.bancoob.componentes.input.*"
			xmlns:containers="br.com.bancoob.componentes.containers.*" 
			xmlns:bancoob="br.com.bancoob.componentes.*"
			xmlns:comum="br.com.sicoob.sisbr.sicoobdda.comumflex.componentes.*"
			paddingLeft="0" paddingRight="0" paddingBottom="0" clipContent="true"
			width="900">
	
	<containers:VBoxBancoob width="100%" height="100%">
		<comum:FieldSet id="fsSelecaoMensagem" title="Seleção Mensagem" width="100%" height="60">
			<mx:Canvas width="100%" height="100%">
				<componentes:Rotulo text="Mensagem: " width="150" textAlign="right" y="5"/>
				<input:ComboBoxFiltro id="cbTipoMensagem" x="160" y="2" width="480" labelField="descTipo" prompt=" Selecione " somenteMaiusculas="false" validarObrigatorio="true"/>
			</mx:Canvas>
		</comum:FieldSet>
		
		<comum:FieldSet id="fsCadastroMensagem" title="Digitação" width="100%" height="100%" minHeight="200"/>
		
		<containers:HBoxBancoob  width="100%">
			<containers:CanvasBancoob width="100%"/>
			<componentes:Botao id="btnSalvar" label="Salvar" />
			<componentes:Botao id="btnLimpar" label="Limpar" />
			<componentes:Botao id="btnFechar" label="Fechar" />
		</containers:HBoxBancoob>	
	</containers:VBoxBancoob>
	
	
</BaseModulo>
