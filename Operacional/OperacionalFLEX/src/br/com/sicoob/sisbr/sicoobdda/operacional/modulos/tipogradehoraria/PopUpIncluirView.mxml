<?xml version="1.0" encoding="utf-8"?>
<cadastro:FormularioCadastroView  xmlns:mx="http://www.adobe.com/2006/mxml" focusEnabled="false" 
								  width="480" height="200"
								  xmlns="br.com.bancoob.componentes.*"
								  xmlns:cadastro="br.com.bancoob.componentes.cadastro.*"
								  xmlns:comumflex="br.com.sicoob.sisbr.sicoobdda.comumflex.componentes.*"
								  xmlns:containers="br.com.bancoob.componentes.containers.*" 
								  xmlns:navegacao="br.com.bancoob.componentes.navegacao.*"
								  xmlns:bancoob="br.com.bancoob.componentes.*"
								  xmlns:janelas="br.com.sicoob.sisbr.sicoobdda.operacional.modulos.tipogradehoraria.janelas.*"
								  xmlns:grid="br.com.bancoob.componentes.grid.*"
								  xmlns:titulo="br.com.bancoob.componentes.titulo.*"
								  xmlns:input="br.com.bancoob.componentes.input.*"
								  xmlns:botoesComumFlex="br.com.sicoob.sisbr.sicoobdda.comumflex.botoes.*"
								  paddingLeft="0" paddingRight="0" paddingBottom="0" clipContent="true" xmlns:monitoracaocip="br.com.sicoob.sisbr.sicoobdda.operacional.modulos.monitoracaocip.*"  >
	
	<containers:VBoxBancoob width="100%" height="100%" left="5" right="15" top="3" bottom="10" verticalScrollPolicy="off">
		<titulo:SubtituloView id="tituloFiltro" texto="Informações Tipos de Grade Horária" left="0" width="100%" top="0"/>
		<containers:HBoxBancoob width="100%">
			<Rotulo id="rotuloCodTipoGradeHoraria" text="Cód. Tipo Grade: " width="100" textAlign="right"/>
			<input:Texto id="codTipoGradeHoraria" width="100" maxChars="5" validarObrigatorio="true" restrict="a-z0-9"/>
		</containers:HBoxBancoob>
		
		<containers:HBoxBancoob width="100%" >
			<Rotulo text="Descrição: " width="100" textAlign="right"/>
			<input:Texto id="descTipoGradeHoraria" width="100%" somenteMaiusculas="false" maxChars="100" validarObrigatorio="true"   />
		</containers:HBoxBancoob>
		
		<containers:HBoxBancoob width="100%">
			<Rotulo text="Subtipo: " width="100" textAlign="right"/>
			<input:Combo id="cmbSubtipo" labelField="descSubTipoGrade" width="100%" editable="false" labelItemOpcional=""  inserirItemOpcional="true" validarObrigatorio="true"/>
		</containers:HBoxBancoob>

		<containers:HBoxBancoob width="100%" >
			<Rotulo text="Grade Origem:" width="100" textAlign="right"/>
			<input:Combo id="cmbGradeOrigem" labelField="codTipoGradeHoraria" width="100%" editable="false" labelItemOpcional="" inserirItemOpcional="true"/>
		</containers:HBoxBancoob>
		
		<containers:HBoxBancoob width="100%" paddingLeft="215" paddingTop="15">
			<bancoob:Botao label="OK" id="btIncluir" verticalCenter="0"  />
			<bancoob:Botao label="Limpar" id="btLimpar" verticalCenter="0"  />
			<bancoob:Botao label="Fechar" id="btFechar" verticalCenter="0"  />
		</containers:HBoxBancoob>			
	</containers:VBoxBancoob>
	
</cadastro:FormularioCadastroView >