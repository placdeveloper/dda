<?xml version="1.0" encoding="utf-8"?>
<containers:CanvasBancoob xmlns:mx="http://www.adobe.com/2006/mxml"
						  xmlns:comum="br.com.sicoob.sisbr.sicoobdda.comumflex.componentes.*"
						  xmlns:containers="br.com.bancoob.componentes.containers.*"
						  xmlns:componentes="br.com.bancoob.componentes.*"
						  xmlns:input="br.com.bancoob.componentes.input.*"
						  width="100%" >
	
	<comum:FieldSet id="fsCadastroGrupo" title="DDA0200" width="100%" height="120">
		<containers:VBoxBancoob width="100%">
			<containers:CanvasBancoob width="100%" height="90">
				<componentes:Rotulo text="Número Ident. Lançamento: " width="147" textAlign="right" y="5"/>
				<input:Texto id="txtNumIdentcLanc" x="157" y="2" width="400" restrict="0123456789" maxChars="19"/>
				<componentes:Rotulo text="Tipo Transação: " width="147" textAlign="right" y="35"/>
				<input:Combo id="cbTipoTransacao" x="157" y="32" width="400" labelField="label" somenteMaiusculas="true" inserirItemOpcional="true"/>
				<componentes:Rotulo text="Tipo Consulta: " width="147" textAlign="right" y="65"/>
				<input:Combo id="cbTipoConsulta" x="157" y="62" width="400" labelField="label" somenteMaiusculas="true" />
				
				<componentes:Rotulo text="Tipo Retorno: " width="120" textAlign="right" y="5" x="580"/>
				<input:Combo id="cbTipoRetorno" y="2" x="710" width="150" labelField="label" somenteMaiusculas="true" />
				<componentes:Rotulo text="Data Início Apuração:"  width="120" textAlign="right" y="35" x="580"/>
				<input:Data id="dtIniApurc" validarObrigatorio="true" y="32" x="710" width="150"/>
				<componentes:Rotulo text="Data Fim Apuração:"  width="120" textAlign="right" y="65" x="580"/>
				<input:Data id="dtFimApurc" validarObrigatorio="true" y="62" x="710" width="150"/>
			</containers:CanvasBancoob>
		</containers:VBoxBancoob>
	</comum:FieldSet>
</containers:CanvasBancoob>
