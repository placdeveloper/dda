<?xml version="1.0" encoding="utf-8"?>
<containers:CanvasBancoob xmlns:mx="http://www.adobe.com/2006/mxml"
						  xmlns:comum="br.com.sicoob.sisbr.sicoobdda.comumflex.componentes.*"
						  xmlns:containers="br.com.bancoob.componentes.containers.*"
						  xmlns:componentes="br.com.bancoob.componentes.*"
						  xmlns:input="br.com.bancoob.componentes.input.*"
						  width="100%" >
	
	<comum:FieldSet id="fsCadastroGrupo" title="DDA0106" width="100%" height="100">
		<containers:VBoxBancoob width="100%">
			<containers:CanvasBancoob width="100%">
				<componentes:Rotulo text="Tipo Consulta: " width="147" textAlign="right" y="5"/>
				<input:Combo id="cbTipoFiltro" x="157" y="2" width="250"  labelField="label" somenteMaiusculas="true" editable="false" validarObrigatorio="true"/>
				
				<containers:CanvasBancoob id="canvasNumCodBarras" enabled="false" visible="false" y="30">
					<componentes:Rotulo text="Código de Barras: " width="147" textAlign="right" y="5"/>
					<input:Texto id="txtNumCodigoBarras" x="157" y="2" width="300" maxChars="44" restrict="0123456789" validarObrigatorio="true"/>
					<componentes:Rotulo text="Situação Boleto:" width="100" textAlign="right" x="460" y="5"/>
					<input:Combo id="cbTipoSituacaoBoleto" x="565" y="2" width="300"  labelField="descSituacaoBoleto" somenteMaiusculas="true" dropdownWidth="350" validarObrigatorio="true"/>
				</containers:CanvasBancoob>
				
				<containers:CanvasBancoob id="canvasNumIdentBoleto" enabled="false" visible="false" y="30">	
					<componentes:Rotulo text="Num Identificação Boleto: " width="147" textAlign="right" y="5"/>
					<input:Texto id="txtNumIdentBoletoInicial" x="157" y="2" width="180" maxChars="19" restrict="0123456789" validarObrigatorio="true"/>
					<componentes:Rotulo text="a" x="340" y="5" width="20" textAlign="center"/>
					<input:Texto id="txtNumIdentBoletoFinal" x="365" y="2" width="180" maxChars="19" restrict="0123456789" validarObrigatorio="true"/>
					<componentes:Rotulo text="Tipo Consulta: " x="560" y="5" width="70" textAlign="center"/>
					<input:Combo id="cbTipoConsultaBoleto" x="635" y="2" width="100"  labelField="descTipoBoletoConsultado" somenteMaiusculas="true" dropdownWidth="100" validarObrigatorio="true"/>
				</containers:CanvasBancoob>
			</containers:CanvasBancoob>
		</containers:VBoxBancoob>
	</comum:FieldSet>
</containers:CanvasBancoob>