					<xs:documentation>
						<cat:InfCampo>	
							<cat:NomeCampo>Número Ultima Sequência Atualização Baixa Operacional</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumUltIdentcBaixaEft" type="NumIdentcTit" minOccurs="0">
