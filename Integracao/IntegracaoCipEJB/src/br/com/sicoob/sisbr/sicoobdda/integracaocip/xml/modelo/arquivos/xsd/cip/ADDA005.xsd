					<xs:documentation>
						<cat:InfCampo>	
							<cat:NomeCampo>Indicador Manutenção Conta Pagador</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>		
			<xs:element name="TpPessoaAgrgd" type="TpPessoa_CodErro">
				<xs:annotation>
