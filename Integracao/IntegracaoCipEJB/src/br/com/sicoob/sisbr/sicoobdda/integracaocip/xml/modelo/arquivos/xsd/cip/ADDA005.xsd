						<cat:InfCampo>
							<cat:NomeCampo>Tipo Pessoa Agregado</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CNPJ_CPFAgrgd" type="CNPJ_CPF_CodErro">
