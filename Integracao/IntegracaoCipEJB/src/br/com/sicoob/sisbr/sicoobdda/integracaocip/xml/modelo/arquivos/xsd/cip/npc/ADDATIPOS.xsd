<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ADDATIPOS" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:cat="http://www.bcb.gov.br/catalogomsg" elementFormDefault="qualified"
	version="9.1.0">

	<xs:annotation>
		<xs:documentation>
			Definição de tipos globais do DDA.
		</xs:documentation>
	</xs:annotation>

	<!-- Definição do ACTCDOC do arquivo PRO -->
	<xs:complexType name="ADDADOCPROComplexType">
		<xs:sequence>
			<xs:element	name="BCARQ" type="BCARQComplexType"/>
		</xs:sequence> 
	</xs:complexType>
	
	<!-- Definição do ADDADOC do arquivo ERR -->
	<xs:complexType name="ADDADOCERRComplexType"> 
		<xs:sequence>
			<xs:element	name="BCARQ" type="BCARQERRComplexType"/>
			<xs:element name="ESTARQ" type="ESTARQSimpleType" minOccurs="0"/>
		</xs:sequence> 
	</xs:complexType>
	
	<!-- Definição do BCARQ do arquivo ERR -->
  	<xs:complexType name="BCARQERRComplexType">
    	<xs:sequence>
      		<xs:element name="NomArq" type="NomArq_CodErro">
        		<xs:annotation>
          			<xs:documentation>
          				<cat:InfCampo>
							<cat:NomeCampo>Nome do Arquivo</cat:NomeCampo>
            			</cat:InfCampo>
          			</xs:documentation>
        		</xs:annotation>
      		</xs:element>
    	</xs:sequence>
  	</xs:complexType>

  	<xs:complexType name="NomArq_CodErro">
  		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>"NomArq" com atributo para retorno de codigo de	erro.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
    	<xs:simpleContent>
      		<xs:extension base="xs:string">
        		<xs:attribute name="CodErro" type="CodErro" use="required"/>
      		</xs:extension>
    	</xs:simpleContent>
  	</xs:complexType>

	<!-- Definição do BCARQ do arquivo -->
	<xs:complexType name="BCARQComplexType">
		<xs:sequence>
			<xs:element name="NomArq" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Nome do Arquivo</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumCtrlEmis" type="ControleIF">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Número Controle do Emissor</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumCtrlDestOr" type="ControleIF" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Número Controle do Destinatário Original</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ISPBEmissor" type="ISPB">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>ISPB Emissor</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ISPBDestinatario" type="ISPB">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>ISPB Destinatário</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DtHrDDA" type="DataHora" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Data Hora DDA</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SitReqDDA" type="SitCons" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Situação Requisição DDA</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndrFlagFim" type="Indr" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Indicador Flag-Fim</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DtMovto" type="Data">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Data Movimento</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Grupo_Seq" type="Grupo_SeqComplexType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Grupo Sequência</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Grupo_SeqComplexType">
		<xs:sequence>
			<xs:element name="NumSeq" type="NumSeq">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Número Sequência</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndrCont" type="Indr">
				<xs:annotation>
					<xs:documentation>
						<cat:InfCampo>
							<cat:NomeCampo>Indicador Continuidade</cat:NomeCampo>
						</cat:InfCampo>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="Agencia">
		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>Número da agência da instituição que atua no sistema financeiro, sem dígito verificador.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Agencia_CodErro">
		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>"Agencia" com atributo para retorno de codigo de	erro.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="Agencia">
				<xs:attribute name="CodErro" type="CodErro" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

