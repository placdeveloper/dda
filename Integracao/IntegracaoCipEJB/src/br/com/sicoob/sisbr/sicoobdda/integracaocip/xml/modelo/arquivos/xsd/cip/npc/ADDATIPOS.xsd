	<xs:simpleType name="CNPJ_CPF">
		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>Número de Identificação da Pessoa Jurídica no Cadastro Nacional de Pessoa Jurídica ou o Número no Cadastro de Pessoa Física. 2 - No caso de transferência para pessoas isentas de CNPJ ou CPF, o campo CNPJ ou CPF deve ser preenchido com zero.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="14"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CNPJ_CPF_CodErro">
		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>"CNPJ_CPF" com atributo para retorno de codigo de erro.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="CNPJ_CPF">
				<xs:attribute name="CodErro" type="CodErro" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="CNPJBase">
		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>CNPJ base: primeiras 8 posições do CNPJ.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base='xs:string'>
	    	<xs:minLength value='8'/>
	    	<xs:maxLength value='8'/>
	    	<xs:pattern value='[0-9]{8}'/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CNPJBase_CodErro">
		<xs:annotation>
			<xs:documentation>
				<cat:InfTipo>
					<cat:DescricaoTipo>"CNPJBase" com atributo para retorno de codigo de erro.</cat:DescricaoTipo>
				</cat:InfTipo>
			</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="CNPJBase">
				<xs:attribute name="CodErro" type="CodErro" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

