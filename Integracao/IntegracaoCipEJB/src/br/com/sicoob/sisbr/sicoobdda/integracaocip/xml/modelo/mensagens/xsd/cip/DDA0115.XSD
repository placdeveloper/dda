<!-- Definicao da mensagem -->
 
<xs:element name='DOC' type='DOCComplexType'>
  <xs:annotation>
    <xs:documentation>
      <cat:InfEvento>
        <cat:Evento>DDA0115 - Participante requisita cancelamento da baixa operacional</cat:Evento>
        <cat:Descricao>Destinado ao Participante Recebedor requisitar o cancelamento da baixa operacional.</cat:Descricao>
        <cat:Observacao>Para detalhamento da utilização da mensagem, observar o manual de operações Nova Plataforma de Cobrança.</cat:Observacao>
        <cat:Servico>Manutenção Cadastral de Boletos de Pagamento</cat:Servico>
        <cat:TipoFluxo>Fluxo11</cat:TipoFluxo>
      </cat:InfEvento>
    </xs:documentation>
  </xs:annotation>
</xs:element>
 
<xs:complexType name='DOCComplexType'>
  <xs:sequence>
    <xs:element name='BCMSG' type='BCMSGComplexType'/>
    <xs:element name='SISMSG' type='SISMSGComplexType'/>
    <xs:element name='USERMSG' type='USERMSGSimpleType' minOccurs='0'/>
  </xs:sequence>
</xs:complexType>
 
